<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2010/07/nuspec.xsd">
  <metadata>
    <id>HockeySDK.WP.Custom</id>
    <version>2.2.3</version>
    <authors>Bit Stadium GmbH</authors>
  <owners>Bit Stadium GmbH</owners>
    <licenseUrl>https://github.com/bitstadium/HockeySDK-Windows/LICENSE.txt</licenseUrl>
    <projectUrl>https://github.com/bitstadium/HockeySDK-Windows</projectUrl>
    <iconUrl>https://rink.hockeyapp.net/images/icon@2x.png</iconUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>
    <description>Use HockeyApp in your WindowsPhone apps. Collect crash reports, provide a feedback-functionality and enable automatic updates for beta versions.</description>
    <language>en-US</language>
    <copyright>Copyright (c) 2014 Bit Stadium GmbH</copyright>
    <tags>crash reports reporting feedback WP8 WP81SL windows phone hockeyapp</tags>

    <dependencies>
      <group targetFramework="wp8">
        <dependency id="HockeySDK.Core" version="[2.2.3]" />
      </group>
    </dependencies>
  </metadata>

  <files>
    <file src="..\bin\wp8\Release\HockeyApp.*" target="lib\wp8" />
    <file src="..\bin\wp8\Release\de\HockeyApp.*" target="lib\wp8\de" />
    <file src="..\bin\wp8\Release\HockeyApp.*" target="lib\wp8" />

    <file src="content\**" target="content" />
    <file src="tools\**" target="tools" />
  </files>
</package>